<% can.each(rendered_spells, function(spell) { %>
  <div class="spell_item" <%= (el) -> el.data('spell', spell) %>>
    <span class="list_left"><%= spell.name %></span>
    <span class="list_right"><%= spell.mana %> mana</span>
    <span class="list_right"><%= spell.ct %> secs</span>
    <span class="list_right"><%= spell.hps %> HPS</span>
  </div>
<% }) %>