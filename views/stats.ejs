<label>Crit Rating</label><input type="input" id="bcrit" value="<%= stats[0].attr('bcrit'); %>"/ >
<label>Haste Rating</label><input type="input" id="bhaste" value="<%= stats[0].attr('bhaste'); %>"/ >
<label>Mastery Rating</label><input type="input" id="bmast" value="<%= stats[0].attr('bmast'); %>"/ >
