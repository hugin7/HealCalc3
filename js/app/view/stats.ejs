<div <%= (el) -> can.data(el, 'stat', stats) %> ><a class="expandOn"><h4>Stats</h4></a>
<div>
<label>Intellect</label><input type="input" id="bint" value="<%= stats.attr('bint'); %>"/ >
<label>Spirit</label><input type="input" id="bspi" value="<%= stats.attr('bspi'); %>"/ >
<label>Crit Rating</label><input type="input" id="bcrit" value="<%= stats.attr('bcrit'); %>"/ >
<label>Haste Rating</label><input type="input" id="bhaste" value="<%= stats.attr('bhaste'); %>"/ >
<label>Mastery Rating</label><input type="input" id="bmast" value="<%= stats.attr('bmast'); %>"/ >
<label>Weapon Sp</label><input type="input" id="bweapon" value="<%= stats.attr('bweapon'); %>"/ >
</div>
</div>
